<block wx:for="{{products}}">
    <van-card 
        price="{{Math.round(item.view)}}"
        desc="{{item.color}}"
        title="{{item.title}}"
        thumb="{{item.image}}"
        data-id="{{item._id}}"
        >
    </van-card>
</block>
